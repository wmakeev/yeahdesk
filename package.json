{
  "name": "yeahdesk",
  "version": "1.0.0",
  "description": "Библиотека для работы с API сервиса Yeahdesk",
  "keywords": [
    "yeahdesk",
    "client",
    "typescript"
  ],
  "scripts": {
    "compile": "tsc",
    "compile:prod": "rm -rf build/ && tsc --build tsconfig.prod.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "npm run lint:fix && npm run compile",
    "build:prod": "rm -rf build/ && npm run lint:fix && npm run compile:prod"
  },
  "author": "Vitaliy V. Makeev",
  "license": "MIT",
  "devDependencies": {
    "@types/blue-tape": "^0.1.33",
    "@types/node": "^12.19.1",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "@typescript-eslint/typescript-estree": "^4.5.0",
    "blue-tape": "^1.0.0",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3",
    "typescript-eslint-language-service": "^4.1.0"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  }
}
